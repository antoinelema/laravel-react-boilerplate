APP_NAME="Prospecto"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=boilerplate
DB_USERNAME=boileruser
DB_PASSWORD=boilerpass
DB_ROOT_PASSWORD=rootpass

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync

CACHE_STORE=file

# Ports pour docker-compose
APP_PORT=8080
VITE_PORT=5173
FORWARD_DB_PORT=3307
FORWARD_REDIS_PORT=6380

# Utilisateur système
WWWUSER=1000
WWWGROUP=1000

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"


VITE_APP_NAME="${APP_NAME}"

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

# API Keys pour la recherche de prospects
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Optionnel: Hunter.io pour enrichissement emails
HUNTER_API_KEY=your-hunter-api-key

# Configuration OpenStreetMap Nominatim
NOMINATIM_USER_AGENT=Prospecto/1.0

# Mode production
EXTERNAL_SERVICES_DEMO_MODE=false

# Configuration d'enrichissement web
ENRICHMENT_ENABLED=true
ENRICHMENT_DEFAULT_REFRESH_DAYS=30
ENRICHMENT_MAX_ATTEMPTS=3
ENRICHMENT_RATE_LIMIT_DELAY=2
ENRICHMENT_MAX_CONTACTS_PER_PROSPECT=10
ENRICHMENT_COMPLETENESS_THRESHOLD=80
ENRICHMENT_AUTO_ENABLED_DEFAULT=true

# Services d'enrichissement - activer/désactiver individuellement
ENRICHMENT_SERVICE_DUCKDUCKGO=true
ENRICHMENT_SERVICE_GOOGLE=true  
ENRICHMENT_SERVICE_UNIVERSAL=true

# Configuration Selenium pour Google Search (optionnel)
SELENIUM_HUB_URL=http://selenium:4444/wd/hub
SELENIUM_BROWSER=chrome
SELENIUM_TIMEOUT=30
